    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.0",
    "@types/passport-jwt": "^3.0.11",
    "@types/passport-local": "^1.0.36",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
